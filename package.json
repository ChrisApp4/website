{
  "name": "HubRise",
  "private": true,
  "description": "New incarnation of HubRise website",
  "version": "0.1.0",
  "author": "Pavel Ermolin <pavel.2@icloud.com>",
  "scripts": {
    "build": "gatsby build",
    "clean": "gatsby clean",
    "start:dev": "gatsby develop",
    "start:prod": "gatsby build && gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\""
  },
  "dependencies": {
    "@mdx-js/mdx": "1.1.4",
    "@mdx-js/react": "1.1.4",
    "font-awesome": "4.7.0",
    "formik": "1.5.8",
    "foundation-sites": "6.5.3",
    "gatsby": "^2.13.45",
    "gatsby-image": "^2.2.7",
    "gatsby-plugin-manifest": "^2.2.4",
    "gatsby-plugin-mdx": "1.0.20",
    "gatsby-plugin-offline": "^2.2.4",
    "gatsby-plugin-react-helmet": "^3.1.2",
    "gatsby-plugin-sass": "2.1.3",
    "gatsby-plugin-sharp": "^2.2.9",
    "gatsby-remark-autolink-headers": "2.1.8",
    "gatsby-source-filesystem": "^2.1.7",
    "gatsby-transformer-sharp": "^2.2.5",
    "i18next": "17.0.13",
    "lint-staged": "9.2.5",
    "motion-ui": "1.2.3",
    "node-sass": "4.12.0",
    "prism-react-renderer": "0.1.7",
    "prismjs": "1.17.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.1",
    "react-i18next": "10.12.2",
    "yup": "0.27.0"
  },
  "devDependencies": {
    "babel-plugin-prismjs": "1.1.1",
    "babel-preset-gatsby": "0.2.8",
    "eslint-plugin-react-hooks": "2.0.1",
    "husky": "3.0.3",
    "prop-types": "15.7.2",
    "standardx": "3.0.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*/**": [
      "standardx --fix",
      "git add"
    ]
  },
  "standardx": {
    "parser": "babel-eslint",
    "plugins": [
      "react-hooks"
    ]
  },
  "eslintConfig": {
    "rules": {
      "react-hooks/rules-of-hooks": "error",
      "react-hooks/exhaustive-deps": "warn"
    }
  },
  "keywords": [
    "gatsby",
    "hubrise"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:HubRise/website.git"
  },
  "bugs": {
    "url": "https://github.com/HubRise/website/issues"
  }
}
